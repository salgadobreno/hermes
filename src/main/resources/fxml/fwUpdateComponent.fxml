<VBox prefHeight="299.0" prefWidth="310.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <children>
      <Pane prefHeight="330.0" prefWidth="310.0" snapToPixel="false">
         <children>
            <Label layoutX="2.0" layoutY="6.0" text="QR Version" />
