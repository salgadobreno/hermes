            <Label fx:id="capacidadeLabel" layoutX="5.0" layoutY="55.0" text="Capacidade:" />
            <Text layoutX="4.0" layoutY="138.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Temporização" />
            <TextField fx:id="tTimerField" layoutX="58.0" layoutY="174.0" prefHeight="22.0" prefWidth="43.0" text="300" />
            <Text layoutX="102.0" layoutY="191.0" strokeType="OUTSIDE" strokeWidth="0.0" text="ms" />
            <Button defaultButton="true" layoutX="15.0" layoutY="217.0" mnemonicParsing="false" onAction="#zoomAndPlayQrs" style="-fx-background-image: url('../images/media-fast-forward.png');">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/media-play-pause-resume.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Text layoutX="13.0" layoutY="164.0" strokeType="OUTSIDE" strokeWidth="0.0" text="t 1º Qr:" />
