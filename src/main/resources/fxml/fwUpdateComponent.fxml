            <TextField fx:id="tPrimeiroQrField" layoutX="58.0" layoutY="149.0" prefHeight="22.0" prefWidth="43.0" text="2000" />
            <Text layoutX="102.0" layoutY="164.0" strokeType="OUTSIDE" strokeWidth="0.0" text="ms" />
            <Button cancelButton="true" layoutX="58.0" layoutY="217.0" mnemonicParsing="false" onAction="#rewind">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true" rotate="180.0">
                     <image>
                        <Image url="@../images/media-fast-forward.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="loadFileButton" alignment="TOP_LEFT" layoutX="92.0" layoutY="22.0" mnemonicParsing="false" onAction="#loadFile" prefHeight="20.0" prefWidth="202.0" text="Carregar arquivo" />
            <TextField fx:id="moduleOffsetField" layoutX="251.0" layoutY="51.0" prefHeight="22.0" prefWidth="43.0" text="2000" />
            <TextField fx:id="challengeField" layoutX="251.0" layoutY="85.0" prefHeight="22.0" prefWidth="43.0" text="2000" />
            <Text layoutX="172.0" layoutY="68.0" strokeType="OUTSIDE" strokeWidth="0.0" text="module offset" />
            <Text layoutX="174.0" layoutY="102.0" strokeType="OUTSIDE" strokeWidth="0.0" text="challenge" />
            <Text layoutX="154.0" layoutY="130.0" strokeType="OUTSIDE" strokeWidth="0.0" text="interruption stuff" />
            <TextField fx:id="inttuptionStuffField" layoutX="251.0" layoutY="113.0" prefHeight="22.0" prefWidth="43.0" text="2000" />
